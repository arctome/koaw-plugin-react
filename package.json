{
  "name": "koaw-plugin-react",
  "version": "0.0.1",
  "description": "SSR plugin for koaw and Cloudflare Pages",
  "main": "dist/_worker.js",
  "dependencies": {
    "koaw-js": "^0.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "wrangler": "0.0.17"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.1.0",
    "@vitejs/plugin-react": "^1.0.7",
    "rollup": "^2.67.3",
    "rollup-plugin-terser": "^7.0.2",
    "vite": "^2.8.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "vite",
    "preview": "npm run build && npx wrangler pages dev ./dist --kv HtmlTpls",
    "build": "npm run build:static && npm run build:worker && cp dist/index.html dist/__template__.html",
    "build:static": "vite build",
    "build:worker": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arctome/koaw-plugin-react.git"
  },
  "keywords": [
    "vite",
    "koaw",
    "react",
    "SSR",
    "pages"
  ],
  "author": "arctome",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arctome/koaw-plugin-react/issues"
  },
  "homepage": "https://github.com/arctome/koaw-plugin-react#readme"
}
